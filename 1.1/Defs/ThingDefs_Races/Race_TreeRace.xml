      <body>TreeBody</body>
      <baseBodySize>1</baseBodySize>
      <baseHealthScale>1.6</baseHealthScale>
      <bloodDef>sap</bloodDef>
      <foodType>OmnivoreHuman</foodType>
      <gestationPeriodDays>45</gestationPeriodDays>
      <fleshType>treeFlesh</fleshType>
      <litterSizeCurve>
