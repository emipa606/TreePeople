              </li>
              <li>
                <weight>2</weight>
                <only>RGBA(0.823,0.411,0.117,1)</only>
              </li>
            </options>
          </alienhaircolorgen>
        </alienPartGenerator>
        <humanRecipeImport>true</humanRecipeImport>
      </generalSettings>
      <graphicPaths>
        <li>
          <body>Pawn/Humanlike/Tree person/Body/</body>
          <head>Pawn/Humanlike/Tree person/Head/</head>
        </li>
      </graphicPaths>
      <hairSettings>
        <hasHair>true</hasHair>
        <getsGreyAt>1500</getsGreyAt>
        <hairTags>
          <li>TreeHair</li>
        </hairTags>
      </hairSettings>
    </alienRace>
    <statBases>
      <MarketValue>3000</MarketValue>
      <MoveSpeed>2</MoveSpeed>
      <Flammability>2</Flammability>
      <ComfyTemperatureMin>-50</ComfyTemperatureMin>
      <ComfyTemperatureMax>100</ComfyTemperatureMax>
      <LeatherAmount>0</LeatherAmount>
      <MeatAmount>0</MeatAmount>
      <PsychicSensitivity>.7</PsychicSensitivity>
      <PlantWorkSpeed>1.5</PlantWorkSpeed>
    </statBases>
    <tools>
      <li>
        <label>left fist</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>8.2</power>
        <cooldownTime>2</cooldownTime>
        <linkedBodyPartsGroup>LeftHand</linkedBodyPartsGroup>
        <surpriseAttack>
          <extraMeleeDamages>
            <li>
              <def>Stun</def>
              <amount>14</amount>
            </li>
          </extraMeleeDamages>
        </surpriseAttack>
      </li>
      <li>
        <label>right fist</label>
