<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <AlienRace.ThingDef_AlienRace ParentName="BasePawn">
    <defName>Alien_Tree</defName>
    <label>tree person</label>
    <description>A thick-skinned slow-moving plant-person hybrid.</description>
    <soundImpactDefault>HitTree</soundImpactDefault>
    <alienRace>
      <generalSettings>
        <maleGenderProbability>.5</maleGenderProbability>
        <immuneToAge>true</immuneToAge>
        <canLayDown>true</canLayDown>
        <!--forcedRaceTraitEntries>
						<li>
							<defName>StrongRoots</defName>
							<chance>15</chance>
						</li>
					</forcedRaceTraitEntries-->
        <disallowedTraits>
          <li>Pyromaniac</li>
          <li>NightOwl</li>
        </disallowedTraits>
        <factionRelations>
          <li>
            <factions>
              <li>TreeFaction</li>
              <li>TreePlayerFaction</li>
            </factions>
            <goodwill>
              <min>-50</min>
              <max>50</max>
            </goodwill>
          </li>
        </factionRelations>
        <maxDamageForSocialfight>6</maxDamageForSocialfight>
        <allowHumanBios>true</allowHumanBios>
        <immuneToXenophobia>false</immuneToXenophobia>
        <alienPartGenerator>
          <aliencrowntypes>
            <li>Average_Normal</li>
            <li>Average_Pointy</li>
            <li>Average_Wide</li>
            <li>Narrow_Normal</li>
            <li>Narrow_Pointy</li>
            <li>Narrow_Wide</li>
          </aliencrowntypes>
          <alienbodytypes>
            <li>Hulk</li>
            <li>Thin</li>
            <li>Male</li>
            <li>Female</li>
          </alienbodytypes>
          <useGenderedHeads>true</useGenderedHeads>
          <useGenderedBodies>false</useGenderedBodies>
          <alienskincolorgen Class="ColorGenerator_Options">
            <options>
              <li>
                <weight>10</weight>
                <only>RGBA(0.38,0.194,0.006,1)</only>
              </li>
              <li>
                <weight>2</weight>
                <only>RGBA(0.529,0.251,0.003,1)</only>
              </li>
              <li>
                <weight>10</weight>
                <only>RGBA(0.647,0.321,0.176,1)</only>
              </li>
              <li>
                <weight>15</weight>
                <only>RGBA(0.68,0.49,0.255,1)</only>
              </li>
            </options>
          </alienskincolorgen>
          <alienhaircolorgen Class="ColorGenerator_Options">
            <options>
              <li>
                <weight>5</weight>
                <only>RGBA(0.0,0.392,0.0,1)</only>
              </li>
              <li>
                <weight>5</weight>
                <only>RGBA(0.133,0.545,0.133,1)</only>
              </li>
              <li>
                <weight>5</weight>
                <only>RGBA(0.333,0.556,0.137,1)</only>
              </li>
              <li>
                <weight>5</weight>
